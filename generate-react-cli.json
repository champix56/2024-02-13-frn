{
  "usesTypeScript": false,
  "usesStyledComponents": false,
  "usesCssModule": false,
  "cssPreprocessor": "css",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/App/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true
    },
    "uifunc":{
      "path": "src/App/components/ui",
      "withStyle":true,
      "withTest":true,
      "withLazy":true,
      "withStory":false,
      "customTemplates":{
        "component":"grc_templates/function/TemplateName.jsx",
        "style":"grc_templates/TemplateName.styles.jsx",
        "test":"grc_templates/function/TemplateName.test.jsx",
        "lazy":"grc_templates/readme.md"
        
      }
    },
    "layoutfunc":{
      "path": "src/App/components/layout",
      "withStyle":true,
      "withTest":true,
      "withLazy":true,
      "withStory":false,
      "customTemplates":{
        "component":"grc_templates/layout/TemplateName.tsx",
        "style":"grc_templates/TemplateName.styles.jsx",
        "test":"grc_templates/layout/TemplateName.test.tsx",
        "lazy":"grc_templates/readme.md"
        
      }
    }
  }
}